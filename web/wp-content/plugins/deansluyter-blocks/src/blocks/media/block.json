{
  "$schema": "https://schemas.wp.org/trunk/block.json",
  "apiVersion": 3,
  "name": "deansluyter/media",
  "version": "0.1.0",
  "title": "Media",
  "category": "widgets",
  "icon": "format-video",
  "description": "A media card block with image, metadata, description, and external link.",
  "keywords": ["media", "audio", "video", "interview"],
  "supports": {
    "html": false
  },
  "attributes": {
    "imageId": {
      "type": "number"
    },
    "imageUrl": {
      "type": "string",
      "default": ""
    },
    "imageAlt": {
      "type": "string",
      "default": ""
    },
    "title": {
      "type": "string",
      "default": ""
    },
    "subtitle": {
      "type": "string",
      "default": ""
    },
    "date": {
      "type": "string",
      "default": ""
    },
    "description": {
      "type": "string",
      "default": ""
    },
    "length": {
      "type": "string",
      "default": ""
    },
    "linkUrl": {
      "type": "string",
      "default": ""
    },
    "linkText": {
      "type": "string",
      "default": ""
    }
  },
  "textdomain": "deansluyter-blocks",
  "editorScript": "file:./index.js",
  "editorStyle": "file:./index.css",
  "style": "file:./style-index.css"
}
